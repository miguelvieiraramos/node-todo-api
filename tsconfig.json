{
    "compilerOptions": {
        "lib": ["es2023"],
        "module": "node16",
        "target": "es2022",
        "rootDir": "src",
        "outDir": "build",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node16",
        "noImplicitAny": true,
    }
}